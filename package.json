  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/tsc --project . && echo OK.",
    "watch": "./node_modules/.bin/tsc --project . --watch",
    "test": "mocha",
    "coverage": "istanbul cover ./node_modules/mocha/bin/_mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Clarence-pan/node-ftp-server-for-docker.git"
  },
  "keywords": [
    "ftp",
    "ftp-server",
    "docker",
    "container"
  ],
  "files": [
    "index.js",
    "cli.js",
    "lib/ftp-server.js",
    "lib/ftp-server.d.ts",
    "lib/docker-fs.js",
    "lib/docker-fs.d.ts",
    "lib/cli.js",
    "lib/cli.d.ts"
  ],
  "bin": "./cli.js",
  "author": "clarencep",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Clarence-pan/node-ftp-server-for-docker/issues"
  },
  "homepage": "https://github.com/Clarence-pan/node-ftp-server-for-docker#readme",
  "devDependencies": {
    "@types/node": "^7.0.12",
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-loader": "^6.4.1",
