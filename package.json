    "babel-preset-env": "^1.3.3",
    "babel-preset-stage-2": "^6.24.1",
    "co": "^4.6.0",
    "ftp": "^0.3.10",
    "glob": "^7.1.1",
    "intelli-espower-loader": "^1.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "power-assert": "^1.4.2",
    "typescript": "^2.2.2"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "cb2p": "^1.1.0",
